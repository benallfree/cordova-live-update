{
  "name": "cordova-live-update",
  "version": "2.0.0",
  "description": "Over the air updates for Cordova",
  "repository": "benallfree/cordova-live-update",
  "main": "./dist/LiveUpdate.js",
  "scripts": {
    "build": "concurrently npm:build-webpack npm:build-cli",
    "build-webpack": "webpack",
    "build-cli": "babel src/bin/index.js -o dist/cli.js",
    "watch": "concurrently npm:watch-webpack npm:watch-cli",
    "watch-webpack": "webpack --watch",
    "watch-cli": "babel -w --verbose src/bin/index.js -o dist/cli.js"
  },
  "author": "Ben Allfree",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.0",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2"
  },
  "bin": {
    "liveupdate": "./dist/cli.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "archiver": "^3.0.0",
    "commander": "^2.19.0",
    "express": "^4.13.3",
    "find-up": "^3.0.0",
    "glob": "^7.1.3",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "watch": "^1.0.2"
  }
}
